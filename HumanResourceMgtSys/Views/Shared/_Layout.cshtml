    <!--[if lt IE 9]>
    < script src = "https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js" ></ script >
    < script src = "https://oss.maxcdn.com/respond/1.4.2/respond.min.js" ></ script >
    < ![endif] -->
    <!--Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic" />

    <script type="text/javascript">
        $(function () {
            // Handler for .ready() called.
            centerTitle();
        });

        $(window).resize(function () {
            centerTitle();
        });

        function centerTitle() {
            var totalWidth = $(window).width();
            if (totalWidth > 1000) {
                var marginleft = ((totalWidth - 700) / 2.5);
                //alert("total = " + totalWidth + " title = 500  Remaining = " + marginleft);
                $("#spanTitle").css("margin-left", marginleft);
                $("#spanTitle").css("display", "block");
            } else if (totalWidth > 820 && totalWidth < 1001) {
                var marginleft = ((totalWidth - 700) / 5.5);
                //alert("total = " + totalWidth + " title = 500  Remaining = " + marginleft);
                $("#spanTitle").css("margin-left", marginleft);
                $("#spanTitle").css("display", "block");
            } else if (totalWidth < 820) {
                var marginleft = ((totalWidth - 700) / 9.5);
                //alert("total = " + totalWidth + " title = 500  Remaining = " + marginleft);
                $("#spanTitle").css("margin-left", 0);
                $("#spanTitle").css("width", "350px");
                $("#spanTitle").css("display", "none");
            }

        }

    </script>

</head>
